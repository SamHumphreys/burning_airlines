<script id="aeroplaneAddTemplate" type='text/template'>
  <h1>Build a plane</h1>
  <label for="Airplane Name: ">Plane name here</label>
  <input type="text" .class="controls" id="aeroplaneName" />
  <label for="Airplane Name: ">number of Rows</label>
  <input type="text" .class="controls"  id="aeroplaneRows"/>
  <label for="Airplane Name: ">Plane name columns</label>
  <input type="text" .class="controls"  id="aeroplaneColumns"/>
  <button class="controls">Create!</button>
</script>

<script id="aeroplaneAddContent" type='text/template'>
  <h4 id="planeName" class="content_h4"></h4>
  <table style="width:100%" class="content_table">
    <tr id="planeRow_content" class="content_table_row">
</script>

<script id="flightsScreen" type="text/template">
  <div>Header stuff here</div>
  <div id="flightsSearch">
    <input id="flightDate" type="date">
    <select id="flightStart"></select>
    <select id="flightEnd"></select>
    <select id="flightPlane"></select>
    <button id="flightCancel">Cancel</button>
    <button id="flightSave">Save</button>
  </div>
  <div>
    <table id="flightsContentTable">
      <tr>
        <th>Date</th>
        <th>Flight</th>
        <th>From/To</th>
        <th>Plane</th>
        <th>Seats</th>
      </tr>
    </table>
  </div>
</script>
